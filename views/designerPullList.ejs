<div class="row">
        <div class="col-md-1"></div>
        <div class="col-md-9">
<table id="dplist" >       
<tr> 
        
    <th align="center">
        <select  name="titles" onchange="getTitle()">
                <option value="playTitle">Production/Play title</option>    
                <% var keys = Object.keys(Events)
                keys.forEach(function(key){ 
                var Name = Events[key].Name %>
                     
                    <option value="playTitle"><%= Name  %> </option> 
                    <% }) %>   
                    <!-- <option value="king">King Lear</option>
                    <option value="show">Senior Showcase</option>
                    <option value="romeo">Romeo Juliet</option>
                    <option value="night">The Nightmare</option> -->
               </select>
        
    </th>  
    <th align="center">
            <select name="names" onchange="getName()">
                 <option value="performName">Performer Name</option>        
                 <option value="kal">Kaleb</option>
                 <option value="elen">Ellen</option>
                 <option value="wat">Watson</option>
                 <option value="kate">Kate</option>
            </select>
    </th> 
    <th>Character Name</th>
    <th>
            <select name="clothing" onchange="getClothingitem()">
                    <option value="clothing">Clothing item</option>        
                    <option value="kal">Tunic</option>
                    <option value="elen">Skirt</option>
                    <option value="wat">Suit</option>
                    <option value="kate">Trousers</option>
               </select>
    </th>
    <th>
            <select name="colors" onchange="getColor()">
                    <option value="clothing">Color</option>        
                    <option value="kal">black</option>
                    <option value="elen">white</option>
                    <option value="wat">cream</option>
                    <option value="kate">tan</option>
                    <option value="kate">brown</option>
                    <option value="kate">grey</option>
                    <option value="kate">red</option>
                    <option value="kate">orange</option>
                    <option value="kate">yellow</option>
                    <option value="kate">green</option>
                    <option value="kate">blue</option>
                    <option value="kate">purple</option>
                    <option value="kate">pink</option>
                    <option value="kate">lavender</option>
                    <option value="kate">dark</option>
                    <option value="kate">light</option>
                    <option value="kate">pale</option>
                    <option value="kate">distressed</option>
                    <option value="kate">old</option>
               </select>
    </th>
    <th>Size</th> 
    <th>Notes</th>   

</tr>
<div class="col-md-2">
        <style>
                
               
                
                
                
                </style>
<tr>
        <td><p id="showTitle"></p></td>
        <td><p id="showName"></p></td>
        <td><p id="character" contenteditable='true'></p></td>
        <td><p id="showclothing"  contenteditable='true'></p></td>
        <td><p id="showcolor"  contenteditable='true'></p></td>
        <td><p id="size"  contenteditable='true'></p></td>
        <td><p id="notes"  contenteditable='true'></p></td>
        <td style="border-right-color: azure; border-bottom-color: azure;border-top-color: azure;"><button id="ro" class="w3-button w3-xlarge w3-circle w3-teal" onclick="addrow()">+</button></td>
        
        
</tr>
</div>
</table>
</div>


